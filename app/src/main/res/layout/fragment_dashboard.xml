<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.DashboardFragment"
    android:background="@color/teal_700">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fontFamily="@font/alata_regular"
        android:gravity="center"
        android:textSize="40dp"
        android:textColor="@color/teal_200"
        android:background="@color/teal_500"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/frame_4"
            android:layout_width="159dp"
            android:layout_height="155dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="24dp"
            app:layout_constraintEnd_toStartOf="@+id/frame_5"
            app:layout_constraintTop_toBottomOf="@+id/frame_2"
            app:srcCompat="@drawable/frame_1" />

        <ImageView
            android:id="@+id/frame_5"
            android:layout_width="159dp"
            android:layout_height="155dp"
            android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/frame_3"
            app:srcCompat="@drawable/frame_1"
            tools:layout_editor_absoluteX="216dp" />

        <ImageView
            android:id="@+id/frame_2"
            android:layout_width="159dp"
            android:layout_height="155dp"
            android:layout_marginTop="36dp"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toStartOf="@+id/frame_3"
            app:layout_constraintHorizontal_bias="0.673"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/frame_1"
            app:srcCompat="@drawable/frame_1" />

        <ImageView
            android:id="@+id/frame_"
            android:layout_width="159dp"
            android:layout_height="155dp"
            android:layout_marginTop="96dp"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/frame_1"
            tools:layout_editor_absoluteX="216dp" />

        <ImageView
            android:id="@+id/frame_3"
            android:layout_width="159dp"
            android:layout_height="155dp"
            android:layout_marginTop="36dp"
            app:layout_constraintTop_toBottomOf="@+id/frame_"
            app:srcCompat="@drawable/frame_1"
            tools:layout_editor_absoluteX="216dp" />

        <ImageView
            android:id="@+id/frame_1"
            android:layout_width="159dp"
            android:layout_height="155dp"
            android:layout_marginTop="96dp"
            android:layout_marginEnd="24dp"
            app:layout_constraintEnd_toStartOf="@+id/frame_"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/frame_1" />
    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>